    <doap:programming-language>java</doap:programming-language>
    <doap:license rdf:resource="http://usefulinc.com/doap/licenses/mpl"/>
    <doap:download-page rdf:resource="http://protege.stanford.edu/plugins/owl/download/protege-owl.zip" dod:content-type="application/x-zip" />

    <doap:repository>
     <doap:SVNRepository>
      <doap:location rdf:resource="http://smi-protege.stanford.edu/repos/protege/owl/trunk/" />
      <doap:browse rdf:resource="http://smi-protege.stanford.edu/svn/owl/trunk/" />
     </doap:SVNRepository>
    </doap:repository>

    <doap:maintainer>
      <foaf:Person rdf:about="http://www.knublauch.com/foaf.rdf#me">
